
.dev-mode
  h1:after, h2:after, h3:after, h4:after, 
  h5:after, h6:after, p:after
    color: inherit
    opacity: .4
    font-size: 14px
    vertical-align: text-top

  h1:after
    content: 'h1'

  h2:after
    content: 'h2'

  h3:after
    content: 'h3'

  h4:after
    content: 'h4'

  h5:after
    content: 'h5'

  h6:after
    content: 'h6'

  p:after
    content: 'p'
